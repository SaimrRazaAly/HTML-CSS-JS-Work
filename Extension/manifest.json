{
    "manifest_version": 3,
    "name": "Stay Focused",
    "version": "1.0",
    "description": "Blocks distracting websites and keeps you on track.",
    "permissions": ["storage", "webRequest", "webRequestBlocking", "tabs"],
    "host_permissions": ["*://*/*"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }
  